{
  "name": "media-manager",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "_hack-first-run": "mkdir -p ./.dev/server/ && touch ./.dev/server/index.js",
    "dev": "export NODE_ENV=development && npm run _hack-first-run && concurrently npm:_watch-server-run npm:_watch-server npm:_watch-static npm:_watch-server-views npm:_watch-client",
    "build": "export NODE_ENV=production && npm run _clean && npm run _build-server && npm run _build-server-views && npm run _build-static && npm run _build-client",
    "prod": "node ./dist/server/index.js",
    "_clean": "rm -rf ./dist/**",
    "_build-static": "sync-files --no-notify-update ./src/static ./dist/static",
    "_watch-static": "sync-files --no-notify-update --watch ./src/static ./.dev/static",
    "_build-server-views": "sync-files ./src/server-views ./dist/server-views",
    "_watch-server-views": "sync-files --watch ./src/server-views ./.dev/server-views",
    "_build-server": "tsc --project ./src/server/tsconfig.json",
    "_watch-server": "tsc --watch --project ./src/server/tsconfig.dev.json",
    "_build-client": "parcel build --no-source-maps ./src/client/client.tsx --out-dir ./dist/static/client",
    "_watch-client": "parcel watch ./src/client/client.tsx --out-dir ./.dev/static/client --public-url ./",
    "_watch-server-run": "nodemon -w ./.dev/server -w ./.dev/server-views ./.dev/server/index.js"
  },
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "clone-deep": "^4.0.1",
    "connect-mongo": "^2.0.3",
    "deep-equal": "^1.1.0",
    "deepmerge": "^3.3.0",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^7.0.1",
    "image-size": "^0.7.4",
    "md5-file": "^4.0.0",
    "mime": "^2.4.4",
    "mongoose": "^5.7.0",
    "multer": "^1.4.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pug": "^2.0.4",
    "qs": "^6.8.0",
    "source-map-support": "^0.5.13",
    "traverse": "^0.6.6",
    "url-parse": "^1.4.7",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.1",
    "@types/clone-deep": "^4.0.1",
    "@types/connect-mongo": "0.0.40",
    "@types/deep-equal": "^1.0.1",
    "@types/dotenv": "^6.1.1",
    "@types/dropzone": "^5.0.6",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.14",
    "@types/fluent-ffmpeg": "^2.1.10",
    "@types/fs-extra": "^5.1.0",
    "@types/image-size": "^0.7.0",
    "@types/md5-file": "^4.0.0",
    "@types/mime": "^2.0.0",
    "@types/multer": "^1.3.9",
    "@types/node": "^10.14.18",
    "@types/passport": "^0.4.7",
    "@types/passport-local": "^1.0.33",
    "@types/pug": "^2.0.4",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/traverse": "^0.6.32",
    "@types/url-parse": "^1.4.3",
    "@types/uuid": "^3.4.5",
    "axios": "^0.18.1",
    "concurrently": "^4.1.2",
    "copy-to-clipboard": "^3.2.0",
    "dropzone": "^5.5.1",
    "less": "^3.10.3",
    "mobx": "^5.13.0",
    "mobx-react": "^5.4.4",
    "nodemon": "^1.19.2",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "shufflejs": "^5.2.3",
    "sync-files": "git+ssh://git@github.com/podgorniy/node-sync-files.git#v1.0.5",
    "typescript": "^3.6.3"
  }
}
